{
  "name": "byjg/anydataset",
  "description": "A data abstraction layer in PHP for access read and write Relational Databases, No Sql, Text, Xml, Json, SparQl and others",
  "authors": [
    {
      "name": "João Gilberto Magalhães",
      "email": "joao@byjg.com.br"
    }
  ],
  "autoload": {
    "psr-4": {
      "ByJG\\AnyDataset\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\AnyDataset\\": "tests/",
      "TestsDb\\AnyDataset\\": "testsdb/"
    }
  },
  "prefer-stable": true,
  "minimum-stability": "dev",
  "require": {
    "php": ">=5.6.0",
    "ext-json": "*",
    "byjg/xmlutil": "1.0.*",
    "byjg/sparqllib": "1.0.*",
    "psr/log": "1.0.*",
    "psr/cache": "1.0.*",
    "byjg/serializer": "1.0.*",
    "byjg/uri": "1.0.*",
    "aws/aws-sdk-php": "3.*"
  },
  "require-dev": {
    "phpunit/phpunit": ">5.7"
  },
  "suggest": {
    "ext-curl": "*",
    "ext-pdo_mysql": "*",
    "ext-pdo_pgsql": "*",
    "ext-mongodb": "*",
    "byjg/cache-engine": "1.0.*"
  },
  "license": "MIT"
}
